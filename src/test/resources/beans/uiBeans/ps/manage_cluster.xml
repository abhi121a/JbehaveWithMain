        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword>DELETE CLUSTER RULE</keyword>
            <element_locator_value>//table[@class='clusterrules']/tbody//th[text()='_##_']/following-sibling::td/span[@class='button button_red deletecluster']</element_locator_value>
        </selector>


        <!--Create Cluster page - Choose file button -->

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword>CHOOSEFILE</keyword>
            <element_locator_value>chooseFile</element_locator_value>
        </selector>

        <!--Create Cluster page - Choose file button -->

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword>UPDATE</keyword>
            <element_locator_value>chooseFile</element_locator_value>
