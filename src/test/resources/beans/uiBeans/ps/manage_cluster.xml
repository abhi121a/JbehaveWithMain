        </selector>

        <selector>
            <element_locator_type>css</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

        <!--Rules - Verifying the name of Cluster in Rules Page -->

        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword>CLUSTER RULE CREATED</keyword>
            <element_locator_value>//table[@class='clusterrules']/tbody//tr/th[contains(text(),'_##_')]</element_locator_value>
        </selector>


