            <element_locator_value>//table[@class='clusterrules']/tbody/tr[2]/td[@class='NCGM']/input[@type='number']</element_locator_value>
        </selector>

<!--need to parameterize the below xpath for all the values-->
        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword>CLUSTER CONSTRAINS</keyword>
            <element_locator_value>//table[@class='clusterrules']//th[contains(text(),'_###_')]/parent::tr/td[@class='_##_']/input</element_locator_value>
        </selector>


        <!--//table[@class='clusterrules']//th[contains(text(),'Cluster001')]/parent::tr/td[@class='CM']/input-->




