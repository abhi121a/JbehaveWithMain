    <Element type="combo">
<!--Locator for selecting Brand Type as All or Selective from dropdown-->
        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword>BRANDTYPE</keyword>
            <element_locator_value>brandType</element_locator_value>
        </selector>
<!--Locator for selecting Seller type as All or Selective from dropdown-->
        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword>SELLERTYPE</keyword>
            <element_locator_value>sellerType</element_locator_value>
        </selector>

<!--locator SUPC upload drop down-->

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword>SUPCTYPE</keyword>
            <element_locator_value>selectSupcType</element_locator_value>
        </selector>

    </Element>

    <Element type="click">

        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword>CREATE CLUSTER</keyword>
            <element_locator_value>//a[@href='/PriceSimulator/exceptions/createclusters.action' and contains(text(),'Create Cluster')]</element_locator_value>
        </selector>


        <!--MANAGE CLUSTER fULFILLMENT TYPE -->

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword>FMTYPE</keyword>
            <element_locator_value>fulfillmentType</element_locator_value>
        </selector>

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword>CREATE CLUSTER BUTTON</keyword>
            <element_locator_value>createClusterButton</element_locator_value>
        </selector>


        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword>DELETE CLUSTER</keyword>
            <element_locator_value>//span [@class='button button_pink delete' and contains(text(),'X')]</element_locator_value>
        </selector>

        <!--Rules - Cluster page tab xpath-->

        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword>RULES CLUSTER</keyword>
            <element_locator_value>//li[@rel='clustertab']</element_locator_value>
        </selector>

        <!--Rules - Cluster page Rule Update-->

        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword>UPDATE CLUSTER RULE</keyword>
            <element_locator_value>//table[@class='clusterrules']/tbody//th[text()='_##_']/following-sibling::td/span[@class='button button_red updatecluster']</element_locator_value>
        </selector>

